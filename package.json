  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc",
    "pretest": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix",
    "test": "nyc mocha -r ts-node/register -r tsconfig-paths/register    src/**/*.spec.ts",
    "test:watch": "mocha -r ts-node/register -r tsconfig-paths/register --extensions ts --watch --watch-files src src/**/*.spec.ts",
    "test:e2e": "NODE_ENV=test  mocha -r ts-node/register -r tsconfig-paths/register  --file ./setup-test.ts --file ./teardown-test.ts  e2e/**/*.e2e.ts ",
    "start": "tsc-watch -p tsconfig.json --onSuccess \"node  -r ./register-paths dist/src/main.js\"",
    "lint": "yarn eslint --ignore-path .gitignore --ignore-path .eslintignore . --fix",
    "lint:file": "yarn eslint --ignore-path .gitignore --ignore-path .eslintignore --fix"
  },
  "dependencies": {
    "@morphic-ts/adt": "^3.0.0",
    "binance-api-node": "^0.10.40",
    "body-parser": "^1.19.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "fp-ts": "^2.10.4",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^1.2.2",
    "io-ts-types": "^0.5.16",
    "monocle-ts": "^2.3.9",
    "mysql": "^2.18.1",
    "newtype-ts": "^0.3.4",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/dotenv-flow": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.41",
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "mocha": "^8.3.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.4"
  }
}
