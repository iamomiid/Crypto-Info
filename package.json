{
  "name": "crypto-info",
  "version": "1.0.0",
  "main": "build/src/main.js",
  "repository": "git@github.com:iamomiid/Crypto-Info.git",
  "author": "Omid Sayfun <seyfanomid@ymail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc",
    "pretest": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix",
    "test": "nyc mocha -r ts-node/register -r tsconfig-paths/register tests/**/*.spec.ts",
    "test:watch": "mocha -r ts-node/register -r tsconfig-paths/register --extensions ts --watch --watch-files src tests/**/*.spec.ts",
    "test:e2e": "NODE_ENV=test  mocha -r ts-node/register -r tsconfig-paths/register  --file ./setup-test.ts --file ./teardown-test.ts  e2e/**/*.spec.ts ",
    "start": "tsc-watch -p tsconfig.json --onSuccess \"node  -r ./register-paths dist/src/main.js\"",
    "lint": "yarn eslint --ignore-path .gitignore --ignore-path .eslintignore . --fix",
    "lint:file": "yarn eslint --ignore-path .gitignore --ignore-path .eslintignore --fix",
    "typeorm": "node -r ts-node/register -r ./register-paths.js ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:generate": "yarn typeorm migration:generate -n",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "@morphic-ts/adt": "^3.0.0",
    "binance-api-node": "^0.10.41",
    "body-parser": "^1.19.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "fp-ts": "^2.10.4",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^1.2.2",
    "io-ts-types": "^0.5.16",
    "monocle-ts": "^2.3.9",
    "mysql": "^2.18.1",
    "newtype-ts": "^0.3.4",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/chai-subset": "^1.3.3",
    "@types/dotenv-flow": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "testdouble": "^3.16.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.4"
  }
}
